// Q1 program to search for particular character in string
 let name = "itee";
 console.log(name.indexOf('t'));

 //Q2 Program to conver minutes to seconds
 let min = 30;
 let sec = min * 60;
 console.log(min + " min into sec is " + sec);

//  Q3 program to search for a element in array of strings
let A = ["itee", " mohit", "mayank", "neeti"];
