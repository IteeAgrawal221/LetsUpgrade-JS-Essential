// Q4 Program to display only elements containing 'a' in the term of array
let array = ["apple", "ant","elephant","left","eagle"];
array.forEach(function(ele){
    console.log(ele);
});

// Q5 Print an array in reverse order
let a =[1,2,3,4,5,6,7,8,9,].reverse();
